<!DOCTYPE html>
<html>
<h2>Users</h2>
<% @users.each do |user| %>
    <li>
      <%= user.name %> Age: <%= user.age %> Email: <%= user.email %>
    </li>
  <% end %>
<h2>Cats</h2>
  <% @cats.each do |cat| %>
    <li>
      Cat <%= cat.name %>
    </li>
  <% end %>
<h2>Todos</h2>
  <% @todos.each do |todo| %>
    <li>
    <p>Task: <%= todo.tasks %> </p> 
    <p>Completed? <%= todo.finished %></p>	
    </li>
  <% end %>
</html>